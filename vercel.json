{
  "version": 2,
  "name": "erp-frontend",
  "builds": [
    {
      "src": "frontend-nextjs/package.json",
      "use": "@vercel/next",
      "config": {
        "zeroConfig": true
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "frontend-nextjs/$1"
    }
  ],
  "github": {
    "silent": true
  }
}
